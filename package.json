    "clean:win": "scripts\\clean-cache.bat",
    "build:clean": "npm run clean && npm run build",
    "migration:check:real-estate": "tsx scripts/check-real-estate-migration.ts",
    "migration:cleanup:real-estate": "tsx scripts/cleanup-real-estate-duplicates.ts",
