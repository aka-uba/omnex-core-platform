    "contracts:send-reminders": "tsx scripts/send-contract-renewal-reminders.ts",
    "payments:generate": "tsx scripts/generate-payments.ts",
    "payments:send-overdue-reminders": "tsx scripts/send-overdue-payment-reminders.ts",
    "schema:merge": "node scripts/merge-schemas.js",
    "schema:validate": "node scripts/validate-tenant-bound.js",
    "schema:validate-relations": "node scripts/validate-relations.js",
    "schema:validate-contracts": "node scripts/validate-module-contracts.js",
    "schema:validate-version": "node scripts/migration-version-check.js",
    "validate:company-tenant-ids": "node scripts/validate-company-tenant-ids.js",
    "validate:api-schema": "node scripts/validate-api-schema-consistency.js",
    "migration:create": "node scripts/migration-helper.js",
    "prisma:generate": "npm run schema:merge && npm run schema:validate && npm run schema:validate-relations && npx prisma generate --schema=prisma/tenant.schema.prisma && npx prisma generate --schema=prisma/core.schema.prisma",
    "prisma:migrate:dev": "npm run schema:merge && npm run schema:validate && npm run schema:validate-relations && npm run schema:validate-contracts && npm run schema:validate-version && npx prisma migrate dev --schema=prisma/tenant.schema.prisma"
  },
